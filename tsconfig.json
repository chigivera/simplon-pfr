{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@ntla9aw/*": ["packages/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
